{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "id": {
        "type": "integer"
      },
      "name": {
        "type": "string"
      },
      "number": {
        "type": "string"
      },
      "balance": {
        "type": "number"
      },
      "currency": {
        "type": "string",
        "enum": ["USD", "RUB"]
      }
    },
    "required": ["id", "name", "number", "balance", "currency"],
    "additionalProperties": false
  }
}